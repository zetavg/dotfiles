                        game_bundle_identifiers = <<-STR
                            [
                                "^com\\\\.nvidia\\\\.gfnpc\\\\.mall",
                                "^com\\\\.ColossalOrder\\\\.CitiesSkylines$",
                                "^com\\\\.scssoft\\\\.ets2$",
                                "^com\\\\.laminar-research\\\\.X-Plane$",
                                "^com\\\\.coffeestainstudios\\\\.goatsimulator"
                            ]
                        STR
