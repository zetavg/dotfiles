{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": true,
        "show_profile_name_in_menu_bar": false
    },
    "profiles": [
        {
            <%
                def fn_keys(additional_config = "", additional_modifiers = "")
                    <<-STR
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f1"#{additional_modifiers}
                            },
                            "to": {
                                "consumer_key_code": "display_brightness_decrement"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f2"#{additional_modifiers}
                            },
                            "to": {
                                "consumer_key_code": "display_brightness_increment"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f3"#{additional_modifiers}
                            },
                            "to": {
                                "key_code": "mission_control"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f4"#{additional_modifiers}
                            },
                            "to": {
                                "key_code": "launchpad"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f5"#{additional_modifiers}
                            },
                            "to": {
                                "consumer_key_code": "dictation"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f6"#{additional_modifiers}
                            },
                            "to": {
                                "key_code": "f6"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f7"#{additional_modifiers}
                            },
                            "to": {
                                "consumer_key_code": "rewind"
                            }
                        },
                        {
                            #{additional_config}
                            "from": {
                                "key_code": "f8"#{additional_modifiers}
                            },
                            "to": {
