                    {
                        "description": "Game Modifications",
                        "manipulators": [
                            <% 12.times do |i| %>
                            {
                                "conditions": [
                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": <%= game_bundle_identifiers %>
                                    }
                                ],
                                "type": "basic",
                                "from": {
                                    "key_code": "f<%= i + 1 %>",
                                    "modifiers": {
                                        "optional": [
                                            "caps_lock",
                                            "control",
                                            "command",
                                            "shift",
                                            "option"
                                        ]
                                    }
                                },
                                "to": {
                                    "key_code": "f<%= i + 1 %>"
                                }
                            },
                            <% end %>

